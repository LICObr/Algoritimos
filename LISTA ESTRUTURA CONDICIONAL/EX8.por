programa {
  funcao inicio(){

real n1,n2
inteiro presenca,qnt

  escreva("Digite suas 2 notas ")
leia(n1,n2)

  escreva("Digite quantas aulas tem e em seguida sua quantidade de presenca ")
  leia(qnt,presenca)

  se ((n1+n2)/2 >= 6 e (presenca/qnt)*100 >= 75)
  escreva("APROVADO")
  senao escreva("REPROVADO")







  }
}
