Algoritmo "alg"

Var
    palavra1, palavra2, pconc: caracter
     i: inteiro

Inicio

    escreva("Digite primeira palavra: ")
    leia(palavra1)
    
    escreva("Digite segunda palavra: ")
    leia(palavra2)
    
    pconc:= palavra1 + " " + palavra2
    
    escreval("a palavra eh: ",pconc)
    
    
    



Fimalgoritmo
