programa {
  funcao inicio() {
    real n1, n2, freq, med
    escreva("Informe a primeira nota: ")
    leia(n1)
    escreva("Informe a segunda nota: ")
    leia(n2)
    escreva("Informe a frequencia: ")
    leia(freq)
    freq = 60/80*100
    med == (n1 + n2)/2
    se (med >=6  e freq >= 60){
      escreva("APROVADO")
    }senao{
      escreva("REPROVADO")
    }







  }
}
