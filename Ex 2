programa {
  funcao inicio() {
    real n1, n2, freq, med
    escreva("Informe a primeira nota: ")
    leia(n1)
    escreva("Informe a segunda nota: ")
    leia(n2)
    escreva("Informe a frequencia: ")
    leia(freq)
    med == (n1 + n2)/2
     se (med <  ou freq < 60){
      escreva("REPROVADO")
    }senao{
      escreva("REPROVADO")
    }







  }
}
